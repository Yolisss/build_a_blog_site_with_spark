{{#partial "content"}}
    <section>
        <div class="container">
            <div class="entry-list">
                <!--                //array of strs, iterates through each item-->
                {{#each entries}}
                    <article>
                        <h2><a href="/detail/{{slug}}">{{ title }}</a></h2>
                        <time datetime="2024-10-01 1:00">{{date}}</time>
                    </article>
                {{/each}}

            </div>
        </div>
    </section>
{{/partial}}


{{> base.hbs }}
